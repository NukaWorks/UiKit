{
  "name": "@powerws/uikit",
  "version": "0.187.0",
  "repository": "https://github.com/PowerWorkspace/UiKit",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf dist/ node_modules/.cache/ ",
    "build": "rm -rf dist/ && mkdir dist/ && tsc && vite build",
    "deploy": "npm version minor --force && npm run build && npm publish --access public",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "bung": "node büng/src/bung.js"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-detect-click-outside": "^1.1.7",
    "react-dom": "^18.2.0",
    "styled-components": "^5.3.6"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@storybook/addon-actions": "^6.5.15",
    "@storybook/addon-essentials": "^6.5.15",
    "@storybook/addon-interactions": "^6.5.15",
    "@storybook/addon-links": "^6.5.15",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/react": "^6.5.15",
    "@storybook/testing-library": "^0.0.13",
    "@types/prop-types": "^15.7.5",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.2",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "@vitejs/plugin-react": "^1.3.0",
    "babel-loader": "^8.2.4",
    "bung": "file:./büng/",
    "eslint": "^8.0.1",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-storybook": "^0.6.8",
    "prettier": "^2.6.2",
    "prop-types": "^15.8.1",
    "sass": "1.50.1",
    "sass-loader": "10.2.1",
    "storybook-dark-mode": "^2.0.4",
    "typescript": "^4.7.4",
    "vite": "^4.0.3",
    "vite-plugin-css-injected-by-js": "^2.0.2",
    "vite-plugin-dts": "^1.4.0"
  },
  "main": "./dist/uikit.umd.js",
  "module": "./dist/uikit.es.js",
  "types": "dist/index.d.ts",
  "files": [
    "/dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/uikit.es.js",
      "require": "./dist/uikit.umd.js"
    },
    "./styling": {
      "import": "./dist/style.css",
      "require": "./dist/style.css"
    }
  },
  "packageManager": "yarn@3.2.3"
}
